      "source": [
        "function norm(x)\n",
        "  beta = maximum(abs.(x))\n",
        "  if beta == 0\n",
        "    return 0\n",
        "  end\n",
        "  else\n",
        "    xhat = (1/beta) * x\n",
        "    sqSum = 0\n",
        "    for idx in xhat\n",
        "      sqSum += idx^2\n",
        "    end\n",
        "     return beta * sqrt(sqSum)\n",
        "  end\n",
        "  return beta\n",
        "end"
      ],
